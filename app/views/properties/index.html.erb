<h1>List of Buildings</h1>

<table>
  <thead>
    <tr><%= notice %></tr>
    <tr>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip Code</th>
    </tr>
  </thead>

  <tbody>
    <% @property.each do |property| %>
      <tr>
        <td><%= property.address %></td>
        <td><%= property.city %></td>
        <td><%= property.state %></td>
        <td><%= property.zip %></td>
        <td><%= property.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Add Building', new_property_path %>
