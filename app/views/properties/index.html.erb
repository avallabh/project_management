<h2>List of Buildings</h2>
<h3><%= notice %></h3>
<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip Code</th>
      <th>Description</th>
      <th>Owner</th>
    </tr>
  </thead>

  <tbody>
    <% @property.each do |property| %>
      <tr>
        <td><%= property.address %></td>
        <td><%= property.city %></td>
        <td><%= property.state %></td>
        <td><%= property.zip %></td>
        <td><%= property.description %></td>
        <td><%= property.owner_id %></td>
      </tr>
    <% end %>
  </tbody>

</table>
<br>
<%= link_to 'Add Building', new_property_path %> |
 <%= link_to 'Add Owner', new_owner_path %>
<br><br>
<%= link_to 'List of Owners', '/owners' %>
